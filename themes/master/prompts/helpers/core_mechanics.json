{
  "description": "This JSON file encapsulates the complete leveling and game mechanics for the Lorelic engine. It includes character progression data, attribute rules, item and economy systems, and challenge benchmarks. It is intended to be a single source of truth for game balance and mechanics implementation.",
  "philosophy": "Provides a theme-agnostic, robust, and flexible framework for character progression and interaction. The design emphasizes player agency through narrative choices, with underlying mechanics providing consistent context for the AI Game Master, ensuring a reactive world and meaningful progression.",
  "attributes": {
    "description": "Defines the four core character attributes. Integrity and Willpower are dynamic resource pools, while Aptitude and Resilience are passive modifiers.",
    "integrity": {
      "id": "INT",
      "name": "Integrity",
      "description": "Represents the entity's capacity to maintain wholeness (physical, mental, structural). Reaching zero means the character is taken out of action for the session.",
      "baseValue": 100,
      "gainPerLevel": 10
    },
    "willpower": {
      "id": "WIL",
      "name": "Willpower",
      "description": "A finite reservoir of energy, resolve, or audacity used to fuel potent abilities and extraordinary actions.",
      "baseValue": 50,
      "gainPerLevel": 5
    },
    "aptitude": {
      "id": "APT",
      "name": "Aptitude",
      "description": "A passive modifier measuring talent, skill, and precision. It influences the quality and degree of success of actions.",
      "baseValue": 10
    },
    "resilience": {
      "id": "RES",
      "name": "Resilience",
      "description": "A defensive modifier reflecting grit, toughness, and the ability to passively endure or mitigate hostile effects.",
      "baseValue": 10
    }
  },
  "progression": {
    "description": "Outlines how characters advance in level. Character level, attributes, and traits are persistent for each theme per user.",
    "levelUpBenefits": {
      "automaticGains": {
        "maxIntegrity": 10,
        "maxWillpower": 5
      },
      "attributePointChoices": {
        "description": "On level-up, the player gains 1 Attribute Point to allocate to one of the following options.",
        "options": {
          "maxIntegrity": {
            "description": "Increase Max Integrity by an additional 20 points.",
            "value": 20
          },
          "maxWillpower": {
            "description": "Increase Max Willpower by an additional 10 points.",
            "value": 10
          },
          "aptitude": {
            "description": "Increase Aptitude by 1 point.",
            "value": 1
          },
          "resilience": {
            "description": "Increase Resilience by 1 point.",
            "value": 1
          }
        }
      },
      "traitSelection": {
        "description": "On level-up, the player is presented with three randomly selected traits relevant to the theme and chooses one to add permanently to their character for that theme."
      }
    }
  },
  "gameplayModifiers": {
    "description": "Dynamic, session-specific modifiers that affect gameplay.",
    "strain": {
      "description": "Represents cumulative physical, mental, or existential toll within the current game session, affecting overall effectiveness. It is session-specific.",
      "levels": [
        {
          "level": 1,
          "name": "Primed/Edged",
          "effect": "+10% to positive outgoing effect magnitudes.",
          "multiplier": 1.1
        },
        {
          "level": 2,
          "name": "Balanced/Steady",
          "effect": "Normal operating effectiveness.",
          "multiplier": 1.0
        },
        {
          "level": 3,
          "name": "Faltering/Frayed",
          "effect": "-10% to positive outgoing effect magnitudes.",
          "multiplier": 0.9
        },
        {
          "level": 4,
          "name": "Broken/Overwhelmed",
          "effect": "-20% to positive outgoing effect magnitudes.",
          "multiplier": 0.8
        }
      ]
    },
    "conditions": {
      "description": "Temporary states (e.g., 'Broken Arm', 'Cursed') imposed by the narrative. They are managed by the AI, apply thematic effects, and are session-specific."
    }
  },
  "economyAndItems": {
    "description": "Rules for the session-based inventory, currency, and equipment systems.",
    "corePrinciples": {
      "inventoryScope": "session-specific",
      "currencyScope": "session-specific",
      "startingGearBasis": "persistent character level"
    },
    "rewardGeneration": {
      "description": "Defines how item and currency rewards are generated and awarded to the player on a session-specific basis.",
      "compendiumBasedItems": {
        "trigger": "AI sets the relevant 'reward_trigger' game state indicator (defined in theme's equipment_slots) to true.",
        "process": "Backend identifies the itemType from the trigger, consults the theme's item compendium, and randomly selects an item appropriate for the player's level using the levelDistribution rules.",
        "levelDistribution": {
          "description": "Rules for selecting a random item's level based on the player's character level. PlayerLvl is clamped to be within the available item levels.",
          "levels_1_10": {
            "playerLvl": 0.40,
            "playerLvlPlus1": 0.35,
            "playerLvlPlus2": 0.15,
            "playerLvlMinus1": 0.10
          },
          "levels_11_25": {
            "playerLvl": 0.50,
            "playerLvlPlus1": 0.25,
            "playerLvlPlus2": 0.08,
            "playerLvlMinus1": 0.17
          },
          "levels_26_45": {
            "playerLvl": 0.60,
            "playerLvlPlus1": 0.15,
            "playerLvlPlus2": 0.05,
            "playerLvlMinus1": 0.20
          },
          "levels_46_50": {
            "playerLvl": 0.70,
            "playerLvlPlus1": 0.05,
            "playerLvlPlus2": 0.03,
            "playerLvlMinus1": 0.22
          }
        }
      },
      "aiGeneratedUniqueItems": {
        "trigger": "AI constructs a complete item data structure and places it into the 'state.pending_generated_item_reward' field.",
        "process": "Backend retrieves the custom item data, adds it to the player's session_inventory, and clears the trigger field.",
        "scope": "current-session only"
      },
      "currency": {
        "trigger": "AI sets the 'reward_trigger' for the 'type: \"money\"' slot to true.",
        "process": "Backend determines an appropriate amount based on the objective's scale (using 'rewardPerStdObj' from the leveling table as a guideline) and adds it to the player's session currency."
      }
    },
    "store": {
      "description": "Defines the mechanics of the in-game store.",
      "unlockLevel": 3,
      "stockRefreshHours": 12,
      "stockSize": 3,
      "sellPriceDescription": "Players can sell session-inventory items for 20-30% of their buyPrice."
    }
  },
  "aiGuidance": {
    "description": "Conceptual formulas to guide the AI's interpretation of mechanics during narrative generation.",
    "aptitudeFormula": {
      "formula": "Base Action Magnitude * (Character_Aptitude / 10)",
      "description": "Guides the AI's judgment of an action's effectiveness and degree of success."
    },
    "resilienceFormula": {
      "formula": "Incoming Negative Magnitude * (10 / Character_Resilience)",
      "description": "Guides the AI's judgment of how well negative consequences are mitigated or resisted."
    }
  },
